{{#with data}}
<div class="preview">
    <div class="preview_header">
        <span class="previev_date">{{date}}</span>
        {{#each category}}
        <span class="previev_category previev_category_{{id}}">{{text}}</span>

        {{/each}}

    </div>

    <strong class="preview_title">{{title}}</strong>

    <p class="preview_description">{{text}}
    </p>
    <a href="#">Continue reading</a>
</div>
{{/with}}